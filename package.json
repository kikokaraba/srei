    "@types/leaflet": "^1.9.8",
    "@types/node": "^22.10.5",
    "@types/pg": "^8.11.10",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/supercluster": "^7.1.3",
    "@typescript-eslint/eslint-plugin": "^8.18.1",
    "@typescript-eslint/parser": "^8.18.1",
    "eslint": "^9.18.0",
    "eslint-config-next": "^16.1.4",
    "postcss": "^8.4.49",
    "prisma": "^7.3.0",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
