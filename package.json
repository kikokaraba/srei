{
  "name": "sria",
  "version": "0.3.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "rm -rf generated node_modules/.prisma 2>/dev/null; rm -rf .next/cache 2>/dev/null || true; npx prisma generate --schema=./prisma/schema.prisma && next build",
    "start": "node .next/standalone/server.js",
    "start:prod": "next start",
    "lint": "next lint",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:init": "bash scripts/init-db.sh",
    "db:auto-seed": "tsx scripts/ensure-admin.ts",
    "cron:start": "tsx scripts/start-cron.ts",
    "postinstall": "prisma generate"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.32.0",
    "@auth/prisma-adapter": "^2.11.1",
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "@tanstack/react-query": "^5.90.16",
    "@types/leaflet.heat": "^0.2.5",
    "@upstash/ratelimit": "^2.0.8",
    "@upstash/redis": "^1.36.1",
    "bcryptjs": "^2.4.3",
    "cheerio": "^1.0.0",
    "leaflet": "^1.9.4",
    "leaflet.heat": "^0.2.0",
    "lucide-react": "^0.562.0",
    "mapbox-gl": "^3.9.0",
    "next": "^16.1.4",
    "next-auth": "^5.0.0-beta.25",
    "pg": "^8.13.1",
    "playwright-core": "^1.49.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-leaflet": "^5.0.0",
    "supercluster": "^8.0.1",
    "tsx": "^4.19.2",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.18",
